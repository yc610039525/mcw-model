		</repository>
	</repositories>

	<properties>
		<version.spring>3.2.5.RELEASE</version.spring>
		<version.slf4j>1.7.2</version.slf4j>
		<version.logback>1.0.13</version.logback>
		<version.aopalliance>1.0</version.aopalliance>
